                if (string.IsNullOrEmpty(submenu.IconClass))
                {
                    submenu.IconClass = "fa fa-circle-o";
                }

                <li><a bo="@(submenu.MenuItemKey)" href="@(submenu.Url)"><i class="@(submenu.IconClass)"></i>  @(submenu.Title)</a></li>
            }
        </ul>
</li>
}
